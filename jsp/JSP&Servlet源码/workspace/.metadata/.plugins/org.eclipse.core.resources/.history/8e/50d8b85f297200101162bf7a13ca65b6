package cc.openhome.tag;

import javax.servlet.jsp.tagext.TagSupport;

public class RemoveTag extends TagSupport {
    private String var;
    private String scope;
    
    public void setVar(String var) {
        this.var = var;
    }
    
    public void setScope(String scope) {
        this.scope = scope;
    }
    
    
}
